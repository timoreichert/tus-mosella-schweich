{
  "name": "tus-mosella-schweich",
  "version": "2.1.0",
  "author": "TuS Mosella Schweich e.V.",
  "homepage": "https://mosella-schweich.de/",
  "scripts": {
    "ng": "ng",
    "android": "npm run android-build && npm run android-sign && npm run android-clean && npm run android-zipalign && npm run android-upload",
    "android-build": "npx ionic cordova build android --prod --release",
    "android-sign": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore ../tus-mosella-app.keystore ./platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk tus-mosella-app",
    "android-clean": "rm ./mosella-app.apk || true",
    "android-zipalign": "$ANDROID_SDK_ROOT/build-tools/28.0.3/zipalign -v 4 ./platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk ./mosella-app.apk",
    "android-upload": "cp mosella-app.apk ~/gdrive-mosella/ && drive push -quiet ~/gdrive-mosella/"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/fire": "^5.1.2",
    "@angular/forms": "^7.2.14",
    "@angular/http": "^7.2.14",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/router": "^7.2.14",
    "@ionic-native/app-version": "^5.4.0",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/firebase-analytics": "^5.5.0",
    "@ionic-native/firebase-crashlytics": "^5.5.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic-native/youtube-video-player": "^5.5.0",
    "@ionic/angular": "^4.1.0",
    "@ionic/storage": "^2.2.0",
    "ajv": "^6.10.0",
    "angularfire2": "^5.1.2",
    "cordova-android": "8.0.0",
    "cordova-plugin-app-version": "0.1.9",
    "cordova-plugin-firebase-crashlytics": "0.1.0",
    "cordova-plugin-youtube-video-player": "2.3.0",
    "cordova-sqlite-storage": "3.2.0",
    "core-js": "^2.5.4",
    "firebase": "^5.9.4",
    "rxjs": "~6.5.1",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.12.3",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/core": "~7.2.3",
    "@angular-devkit/schematics": "~7.2.3",
    "@angular/cli": "~7.3.1",
    "@angular/compiler": "~7.2.14",
    "@angular/compiler-cli": "~7.2.14",
    "@angular/language-service": "~7.2.14",
    "@ionic/angular-toolkit": "~1.4.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.14.2",
    "codelyzer": "~4.5.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^3.1.2",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.0.0",
    "tslint": "~5.12.0",
    "typescript": "~3.2.4"
  },
  "description": "Ofizielle App des TuS Mosella Schweich e.V.",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-app-version": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-youtube-video-player": {},
      "cordova-plugin-firebase-analytics": {},
      "cordova-plugin-firebase-crashlytics": {
        "ANDROID_FIREBASE_CORE_VERSION": "16.0.8"
      }
    },
    "platforms": [
      "android"
    ]
  }
}
